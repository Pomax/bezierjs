{
  "name": "bezier-js",
  "version": "2.2.2",
  "description": "A javascript library for working with Bezier curves",
  "main": "./index.js",
  "scripts": {
    "test": "npm run build && npm run test:all",
    "test:oo": "node test/test.js",
    "test:fp": "node test/test.fp.js",
    "test:all": "npm run test:oo && npm run test:fp",
    "build:oo": "rimraf bezier.js && webpack index.js",
    "build:fp": "rimraf fp/bezier.js && webpack fp/index.js --config fp/webpack.config.js",
    "build": "npm run build:oo && npm run build:fp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pomax/bezierjs"
  },
  "keywords": [
    "bezier",
    "curves"
  ],
  "author": "Pomax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pomax/bezierjs/issues"
  },
  "homepage": "https://github.com/Pomax/bezierjs",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "browserify": "^9.0.3",
    "chai": "^3.2.0",
    "chai-stats": "^0.3.0",
    "deep-freeze": "0.0.1",
    "mkdirp": "^0.5.1",
    "uglify": "^0.1.1",
    "uglify-js": "^2.4.17",
    "webpack": "^1.12.13"
  }
}
